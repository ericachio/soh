<body>
<center>

	<div id="header">
		<h1> This is Exercise This is Exercise </h1>
	</div>

	<div id="follow">
		<h2 > Exercise Exercise Exercise</h2>
	</div>


	{{#if error}}
   		<h3 class="alert">{{error}}</h3>
	{{/if}}

	{{#if firstS}}
		<form method="GET" action="/pickexercise" class="bform">
			<select name="strengthexercise">
				<option value="">--Please choose an option--</option>
					<optgroup label="Top Strengths">
						<option value="{{firstS}}"> {{firstS}} </option>
						<option value="{{secondS}}"> {{secondS}} </option>
						<option value="{{thirdS}}"> {{thirdS}} </option>
					</optgroup>
					<optgroup label="Bottom Strengths">
						<option value="{{firstW}}"> {{firstW}} </option>
						<option value="{{secondW}}"> {{secondW}} </option>
						<option value="{{thirdW}}"> {{thirdW}} </option>
					</optgroup>
			</select>
			<input class="submitbutton" type="submit">
		</form>
		
	{{/if}}
	<a href="/homepage" class="backbutton"><p style="font-size:20px;">Back<p></a>

</center>
</body>
